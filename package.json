{
	"name": "sendit",
	"version": "1.0.0",
	"description": "A courier service project",
	"main": "app.js",
	"engines": {
		"node": "8.11.2"
	},
	"scripts": {
		"build": "babel ./ -d build",
		"start": "babel-node server/server.js",
		"start-dev": "nodemon ./server/server.js --exec babel-node --presets es2015",
		"parcelTable": "node ./server/models/parcelsdb createTables",
		"userTable": "node ./server/models/userdb createTables",
		"test": "nyc mocha --exit --timeout 30000 --compilers js:babel-core/register ./server/test/*.js",
		"coverage": "nyc report --reporter=text-lcov | coveralls",
		"lint": "./node_modules/.bin/eslint ./server --config .eslintrc.json",
		"lint-fix": "./node_modules/.bin/eslint ./server --fix .eslintrc.json"
	},
	"repository": {
		"type": "git",
