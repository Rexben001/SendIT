{
	"name": "sendit",
	"version": "1.0.0",
	"description": "A courier service project",
	"main": "app.js",
	"scripts": {
		"start": "nodemon app.js --exec babel-node --presets es2015",
		"lint": "eslint **/*.js --ignore-pattern node_modules/",
		"test": "mocha --timeout 15000 --compilers js:babel-core/register ./test/app.js",
		"cover": "babel-node ./node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha -- -R spec test/app.js",
		"coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Rexben001/SendIT.git"
	},
	"author": "Rexben",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Rexben001/SendIT/issues"
	},
	"homepage": "https://github.com/Rexben001/SendIT#readme",
	"devDependencies": {
		"chai": "^4.2.0",
		"coveralls": "^3.0.2",
		"eslint": "^5.8.0",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-plugin-import": "^2.14.0",
		"istanbul": "^0.4.5",
		"mocha": "^5.2.0",
		"nodemon": "^1.18.6",
		"should": "^13.2.3",
		"sinon": "^7.1.1",
		"superagent": "^4.0.0-beta.5",
		"supertest": "^3.3.0"
	},
	"dependencies": {
		"babel-cli": "^6.26.0",
		"babel-eslint": "^10.0.1",
		"babel-istanbul": "^0.12.2",
		"babel-preset-es2015": "^6.24.1",
		"body-parser": "^1.18.3",
		"chai-http": "^4.2.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-plugin-jsx-a11y": "^6.1.2",
		"eslint-plugin-react": "^7.11.1",
		"express": "^4.16.4",
		"mocha-lcov-reporter": "^1.3.0"
	}
}
